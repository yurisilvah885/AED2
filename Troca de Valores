#include <stdio.h>

void troca(int *x, int *y){
    int aux;
    
    //ENTRADAS
    /*  
    aux = a;
    a = b;
    b = aux;
    */
    
    aux = *x;
    *x = *y;
    *y = aux;
}
int main()
{
    int a,b;
   printf("A: ");
   scanf("%d", &a); 
   printf("B: ");
   scanf("%d", &b);
   
   troca(&a,&b);
   
   printf("a: %d", a);
   printf("b: %d", b);




    return 0;
}

